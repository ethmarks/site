{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/common",
      "destination": "/common/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/common/",
      "destination": "https://common-ethmarks.vercel.app/"
    },
    {
      "source": "/common/:path*",
      "destination": "https://common-ethmarks.vercel.app/:path*"
    }
  ]
}

{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/common",
      "destination": "/common/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/common/",
      "destination": "https://common-ethmarks.vercel.app/"
    },
    {
      "source": "/common/:path*",
      "destination": "https://common-ethmarks.vercel.app/:path*"
    },
    {
      "source": "/about/:path*",
      "destination": "https://about-ethmarks.vercel.app/about/:path*"
    },
    {
      "source": "/posts/:path*",
      "destination": "https://posts-ethmarks.vercel.app/posts/:path*"
    }
  ]
}
